{
    "name": "spresnac/laravel-helper-bunch",
    "license": "MIT",
    "description": "Some useful helper for laravel",
    "keywords": ["php", "laravel"],
    "type": "library",
    "require": {
      "php": "^7.2",
      "illuminate/support": "5.*|^6.0|7.*|8.*",
      "illuminate/console": "5.*|^6.0|7.*|8.*",
      "illuminate/pagination": "5.*|^6.0|7.*|8.*"
    },
    "require-dev": {
      "orchestra/testbench": "4.* || 5.* || 6.*",
      "laravel/framework": "6.*||7.*||8.*"
    },
    "funding": [
      {
        "type": "other",
        "url": "https://www.buymeacoffee.com/spresnac"
      },
      {
        "type": "paypal",
        "url": "https://www.paypal.me/paladin"
      }
    ],
    "autoload": {
      "psr-4": {
        "spresnac\\": "src/spresnac/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "spresnac\\Helper\\tests\\": "tests/spresnac/"
      }
    },
    "minimum-stability": "stable",
    "authors": [
      {
        "name": "Sascha Presnac",
        "email": "presnac@gmail.com"
      }
    ],
    "homepage": "https://github.com/spresnac/laravel-helper-bunch",
    "support": {
      "issues": "https://github.com/spresnac/laravel-helper-bunch/issues",
      "source": "https://github.com/spresnac/laravel-helper-bunch"
    },
    "extra": {
      "laravel": {
        "providers": [
          "spresnac\\Helper\\HelperBunchCommandServiceProvider"
        ]
      }
    },
    "scripts": {
      "test-ci": "phpunit --coverage-clover=coverage.xml"
    }
}
