{
    "name": "spresnac/laravel-helper-bunch",
    "license": "MIT",
    "description": "Some usefull helper for laravel",
    "keywords": ["php", "laravel"],
    "type": "library",
    "require": {
      "php": ">=7.2.0",
      "illuminate/support": "5.*|^6.0",
      "illuminate/console": "5.*|^6.0",
      "illuminate/pagination": "5.*|^6.0"
    },
    "require-dev": {
      "phpunit/phpunit": "7.*|^8.0"
  },
    "autoload": {
      "psr-4": {
        "spresnac\\Helper\\": "src/spresnac/Helper/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "spresnac\\Helper\\tests\\": "tests/Helper/"
      }
    },
    "minimum-stability": "stable",
    "authors": [
      {
        "name": "Sascha Presnac",
        "email": "presnac@gmail.com"
      }
    ],
    "homepage": "https://github.com/spresnac/laravel-helper-bunch",
    "support": {
      "issues": "https://github.com/spresnac/laravel-helper-bunch/issues",
      "source": "https://github.com/spresnac/laravel-helper-bunch"
    },
    "scripts": {
      "test-ci": "phpunit --coverage-clover=coverage.xml"
    }
}
